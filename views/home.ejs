<%- include('partials/header') -%>

<h1>Home</h1>

<p><%= homeStartingContent %></p>

<% posts.forEach((post)=>{ %>
    <h2><%= post.postTitle %></h2>
    <div>
        <p><%= post.postData.substring(0,100)+"..." %><a href="/posts/<%= post.postTitle %>">Read more</a></p>
    </div>
<% }) %>

<%- include('partials/footer') -%>
<script src="/js/index.js"></script>